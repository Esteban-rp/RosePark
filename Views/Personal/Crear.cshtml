<h2>Creaci√≥n de Usuario</h2>
<a asp-action="Index" class="btn btn-warning">Cancelar</a>

@model Persona
@{
    ViewData["Title"] = "Crear";
}

<form asp-action="CrearPersonal" method="post">
    <div class="form-group">
        <label asp-for="Nombres"></label>
        <input required asp-for="Nombres" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Apellidos"></label>
        <input required asp-for="Apellidos" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Telefono"></label>
        <input required asp-for="Telefono" class="form-control"/>
    </div>

    <div class="form-group">
        <label asp-for="Correo"></label>
        <input required asp-for="Correo" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Contrasena"></label>
        <input asp-for="Contrasena" required class="form-control" />
    </div>
    <div class="form-group text-center">
        <button type=submit class="btn btn-primary mt-4">Enviar</button>
    </div>
</form>
